  object FDQryMesas: TFDQuery
    Connection = DmBD.DBConexao
    SQL.Strings = (
      'SELECT'
      'A.CODIGO,'
      'A.DESCRICAO,'
      #39'OCUPADA'#39' AS STATUS_MESA'
      'FROM MESAS A JOIN'
      'COMANDAS   B ON B.MESA = A.CODIGO'
      'WHERE B.STATUS = '#39'A'#39
      '  AND B.MESA IS NOT NULL '
      ''
      'UNION'
      ''
      'SELECT'
      'A.CODIGO,'
      'A.DESCRICAO,'
      #39'LIVRE'#39'      AS STATUS_MESA'
      'FROM MESAS A '
      'WHERE A.CODIGO NOT IN ('
      'SELECT'
      'A.CODIGO'
      'FROM MESAS A JOIN'
      'COMANDAS B ON B.MESA = A.CODIGO'
      'WHERE B.STATUS = '#39'A'#39
      'AND B.MESA IS NOT NULL '
      ')'
      'ORDER BY A.DESCRICAO')
    Left = 288
    Top = 192
    object FDQryMesasCODIGO: TIntegerField
      FieldName = 'CODIGO'
      Origin = 'CODIGO'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      ReadOnly = True
      Required = True
    end
    object FDQryMesasDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Origin = 'DESCRICAO'
      Required = True
    end
    object FDQryMesasSTATUS_MESA: TStringField
      FieldName = 'STATUS_MESA'
      Origin = 'STATUS_MESA'
      ReadOnly = True
      Required = True
      Size = 7
    end
  end
  object DataSource: TDataSource
    DataSet = FDQryMesas
    Left = 352
    Top = 192
  end
  object FDQryCom: TFDQuery
    MasterSource = DataSource
    MasterFields = 'CODIGO'
    Connection = DmBD.DBConexao
    SQL.Strings = (
      'SELECT '
      'D.DESCRICAO AS MESA,'
      'A.DATA_HORA,'
      'B.VALOR,'
      'C.NOME      AS GARCOM'
      'FROM COMANDAS A'
      'LEFT JOIN COMANDAS_ITENS B ON B.COMANDA = A.CODIGO'
      '     JOIN GARCONS        C ON C.CODIGO  = A.GARCOM'
      '     JOIN MESAS          D ON D.CODIGO  = A.MESA'
      'WHERE A.CODIGO = :CODIGO')
    Left = 328
    Top = 304
    ParamData = <
      item
        Name = 'CODIGO'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
    object FDQryComMESA: TStringField
      FieldName = 'MESA'
      Origin = 'MESA'
      Required = True
    end
    object FDQryComGARCOM: TStringField
      FieldName = 'GARCOM'
      Origin = 'GARCOM'
      Required = True
      Size = 45
    end
    object FDQryComDATA_HORA: TSQLTimeStampField
      FieldName = 'DATA_HORA'
      Origin = 'DATA_HORA'
    end
    object FDQryComVALOR: TBCDField
      FieldName = 'VALOR'
      Origin = 'VALOR'
      currency = True
      Precision = 15
      Size = 2
    end
  end
