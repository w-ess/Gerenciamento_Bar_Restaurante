  object FDQryMesas: TFDQuery
    Connection = DmBD.DBConexao
    SQL.Strings = (
      'SELECT'
      'B.CODIGO AS COMANDA,'
      'A.CODIGO,'
      'A.DESCRICAO,'
      #39'OCUPADA'#39' AS STATUS_MESA'
      'FROM MESAS A JOIN'
      'COMANDAS   B ON B.MESA = A.CODIGO'
      'WHERE B.STATUS = '#39'A'#39
      '  AND B.MESA IS NOT NULL '
      ''
      'UNION'
      ''
      'SELECT'
      'NULL AS COMANDA,'
      'A.CODIGO,'
      'A.DESCRICAO,'
      #39'LIVRE'#39'      AS STATUS_MESA'
      'FROM MESAS A '
      'WHERE A.CODIGO NOT IN ('
      'SELECT'
      'A.CODIGO'
      'FROM MESAS A JOIN'
      'COMANDAS B ON B.MESA = A.CODIGO'
      'WHERE B.STATUS = '#39'A'#39
      'AND B.MESA IS NOT NULL '
      ')'
      'ORDER BY A.DESCRICAO')
    Left = 288
    Top = 192
