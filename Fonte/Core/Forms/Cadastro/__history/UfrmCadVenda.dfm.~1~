      Precision = 15
      Size = 0
    end
    object FDQryItemComandaTOTAL: TCurrencyField
      FieldKind = fkCalculated
      FieldName = 'TOTAL'
      Calculated = True
    end
  end
  object DsComanda: TDataSource
