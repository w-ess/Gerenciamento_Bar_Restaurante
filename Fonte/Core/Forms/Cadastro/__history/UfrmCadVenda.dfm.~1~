    OnCalcFields = FDQryItemComandaCalcFields
    CachedUpdates = True
    IndexFieldNames = 'COMANDA'
    MasterSource = DsComanda
    MasterFields = 'CODIGO'
    DetailFields = 'COMANDA'
    Connection = DmBD.DBConexao
    SchemaAdapter = FDSchemaAdapter
    FetchOptions.AssignedValues = [evDetailCascade]
    FetchOptions.DetailCascade = True
    SQL.Strings = (
      'SELECT ci.CODIGO, ci.COMANDA, ci.PRODUTO,'
      '       pr.DESCRICAO, ci.VALOR, ci.QUANTIDADE, ci.DESCONTO  '
      'FROM COMANDAS_ITENS ci'
      '  INNER JOIN PRODUTOS pr ON ci.PRODUTO = pr.CODIGO'
      'WHERE COMANDA = :CODIGO'
      ''
      '')
    Left = 360
    Top = 232
    ParamData = <
      item
        Name = 'CODIGO'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
    object FDQryItemComandaCODIGO: TFDAutoIncField
      FieldName = 'CODIGO'
      Origin = 'CODIGO'
      ProviderFlags = [pfInWhere, pfInKey]
      ReadOnly = True
    end
    object FDQryItemComandaCOMANDA: TIntegerField
      FieldName = 'COMANDA'
      Origin = 'COMANDA'
      Required = True
    end
    object FDQryItemComandaPRODUTO: TIntegerField
      FieldName = 'PRODUTO'
      Origin = 'PRODUTO'
      Required = True
    end
    object FDQryItemComandaDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Origin = 'DESCRICAO'
      ProviderFlags = [pfInWhere]
      Size = 45
    end
    object FDQryItemComandaVALOR: TBCDField
      FieldName = 'VALOR'
      Origin = 'VALOR'
      currency = True
      Precision = 15
      Size = 2
    end
    object FDQryItemComandaQUANTIDADE: TBCDField
      FieldName = 'QUANTIDADE'
      Origin = 'QUANTIDADE'
      Precision = 15
      Size = 0
    end
    object FDQryItemComandaDESCONTO: TBCDField
      FieldName = 'DESCONTO'
      Origin = 'DESCONTO'
