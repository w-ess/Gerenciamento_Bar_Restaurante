  object FDQryComanda: TFDQuery
    CachedUpdates = True
    Connection = DmBD.DBConexao
    SQL.Strings = (
      'Select c.CODIGO, c.DATA_HORA, C.GARCOM, g.NOME, '
      '       C.MESA, m.DESCRICAO'
      'From COMANDAS c'
      '  Inner Join GARCONS g On c.GARCOM = g.CODIGO'
      '  Inner Join MESAS m On c.MESA = m.CODIGO'
